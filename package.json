{
  "name": "donate.shpp.com-frontend",
  "version": "1.0.0",
  "description": "Frontend for the service donate.shpp.com",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:watch": "better-npm-run build:watch",
    "lint": "node_modules/.bin/eslint frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onix-Systems/donate.shpp.com-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Onix-Systems/donate.shpp.com-frontend/issues"
  },
  "homepage": "https://github.com/Onix-Systems/donate.shpp.com-frontend#readme",
  "betterScripts": {
    "build:dev": {
      "command": "webpack --config webpack/webpack.dev.config.js ",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack -p --config webpack/webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:watch": {
      "command": "webpack --config webpack/webpack.dev.config.js --watch",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "^0.1.1",
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
